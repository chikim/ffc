<div>
  <%= form_for photo, url: {controller: :photos, action: (params[:id].present? ? :update : :create)} do |f| %>
    <p>
    <%= f.label :title %>
    </br>
    <%= f.text_field :title %>
    </p>

    <p>
    <%= f.label :image %>
    </br>
    <%= f.file_field :image %>
    </p>

    <p><%= f.submit %></p>
  <% end %>
</div>
